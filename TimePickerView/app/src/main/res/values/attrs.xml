<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundReleaseColor" format="color"/>
        <attr name="roundPressColor" format="color"/>
        <attr name="roundBetweenDistance" format="dimension"/>
        <attr name="roundWaveBetweenDistance" format="dimension"/>
        <attr name="roundWidth" format="dimension"/>
        <attr name="tipTextSize" format="dimension"/>
        <attr name="urgentTextSize" format="dimension"/>
        <attr name="counterTimeTextSize" format="dimension"/>
        <attr name="counterMinTextSize" format="dimension"/>
        <attr name="counterUnitTextSize" format="dimension"/>
        <attr name="roundProgressMax" format="integer"/>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="AudioRecorderView">
        <attr name="arv_roundColor" format="color"/>
        <attr name="arv_roundProgressColor" format="color"/>
        <attr name="arv_roundWidth" format="dimension"/>
        <attr name="arv_textSize" format="dimension"/>
        <attr name="arv_counterTimeTextSize" format="dimension"/>
        <attr name="arv_max" format="integer"/>
        <attr name="arv_style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="ViewFlow">
        <attr name="sidebuffer" format="integer" />
    </declare-styleable>

    <declare-styleable name="CircleFlowIndicator">
        <attr name="activeColor" format="color" />
        <attr name="inactiveColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="strokewidth" format="dimension" />
        <attr name="centered" format="boolean" />
        <attr name="fadeOut" format="integer" />
        <attr name="inactiveType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="activeType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TitleFlowIndicator">
        <attr name="titlePadding" format="dimension" />
        <!-- Left/right padding of not active view titles. -->
        <attr name="clipPadding" format="dimension" />
        <attr name="selectedColor" format="color" />
        <attr name="selectedBold" format="boolean" />
        <attr name="selectedSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="footerLineHeight" format="dimension" />
        <attr name="footerColor" format="color" />
        <attr name="footerTriangleHeight" format="dimension" />
        <attr name="customTypeface" format="string" />
    </declare-styleable>

    <declare-styleable name="PageControlView">
        <attr name="focused_src" format="reference"/>
        <attr name="unfocused_src" format="reference"/>
        <attr name="cell_width" format="reference|dimension"/>
        <attr name="cell_height" format="reference|dimension"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="circle_image_view_border_width" format="dimension" />
        <attr name="circle_image_view_border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularProgress">
        <attr name="circular_progress_borderWidth" format="dimension" />
        <attr name="circular_progress_arrowColor" format="color" />
        <attr name="circular_progress_progressCircleColor" format="color" />
        <attr name="circular_progress_progressCircleBgColor" format="color" />
        <attr name="circular_progress_crossLineColor" format="color" />
    </declare-styleable>

    <declare-styleable name="DynamicCircle">
        <attr name="normalSize" format="dimension" />
        <attr name="bigSize" format="dimension" />
        <attr name="normalFontSize" format="dimension" />
        <attr name="bigFontSize" format="dimension" />
        <attr name="bgColor" format="color"/>
        <attr name="numColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="DynamicLine">
        <attr name="dynamic_line_bgColor" format="color"/>
        <attr name="dynamic_line_dynamicColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RecordWave">
        <attr name="record_wave_line_color" format="color"/>
        <attr name="record_wave_width" format="dimension"/>
        <attr name="record_wave_height" format="dimension"/>
        <attr name="record_wave_rect_width" format="dimension"/>
        <attr name="record_wave_rect_distance" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="WaterWave">
        <attr name="waveRadius" format="dimension" />
        <attr name="betweenRadius" format="dimension" />
        <attr name="centerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CountDownTextView">
        <attr name="countDown_textColor" format="color"/>
        <attr name="countDown_textSize" format="dimension"/>
        <attr name="countDown_bgColor" format="color"/>
        <attr name="countDown_description" format="string"/>
    </declare-styleable>

    <declare-styleable name="mRippleView">
        <attr name="mRippleColor" format="color" />
        <attr name="mAlphaFactor" format="float" />
        <attr name="hover" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ScaleViewGroup">
        <attr name="marginLeftRight" format="dimension"></attr>
        <attr name="gutterSize" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="ScaleViewPager">
        <attr name="offscreenPageLimit" format="integer" />
        <attr name="pagerMargin" format="dimension" />
        <attr name="paddingLeft" format="dimension" />
        <attr name="paddingRight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ToggleButton">
        <attr name="toggleButton_borderWidth" format="dimension" />
        <attr name="toggleButton_offBorderColor" format="reference|color" />
        <attr name="toggleButton_offColor" format="reference|color" />
        <attr name="toggleButton_onColor" format="reference|color" />
        <attr name="toggleButton_spotColor" format="reference|color" />
        <attr name="toggleButton_animate" format="reference|boolean" />
        <attr name="toggleButton_isDefaultOn" format="reference|boolean" />
    </declare-styleable>

    <declare-styleable name="ToggleImageButton">
        <attr name="tib_drawable_on" format="reference" />
        <attr name="tib_drawable_off" format="reference" />
    </declare-styleable>

    <declare-styleable name="RecyclerViewPager">
        <attr name="rvp_triggerOffset" format="float" />
        <attr name="rvp_flingFactor" format="float" />
        <attr name="rvp_singlePageFling" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleIndicator">
        <attr name="ci_radius" format="dimension"/>
        <attr name="ci_margin" format="dimension"/>
        <attr name="ci_background" format="color|integer"/>
        <attr name="ci_selected_background" format="color|integer"/>
        <attr name="ci_gravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="ci_mode">
            <enum name="inside" value="0"/>
            <enum name="outside" value="1"/>
            <enum name="solo" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="BubbleView">
        <attr name="arrowWidth" format="dimension" />
        <attr name="angle" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
        <attr name="arrowPosition" format="dimension" />
        <attr name="bubbleColor" format="color" />
        <attr name="arrowCenter" format="boolean" />
        <attr name="arrowLocation" format="enum">
            <enum name="left" value="0x00" />
            <enum name="right" value="0x01" />
            <enum name="top" value="0x02" />
            <enum name="bottom" value="0x03" />
        </attr>
    </declare-styleable>
    <declare-styleable name="pickerview">
        <attr name="pickerview_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="pickerview_textSize" format="dimension"/>
        <attr name="pickerview_textColorOut" format="color"/>
        <attr name="pickerview_textColorCenter" format="color"/>
        <attr name="pickerview_dividerColor" format="color"/>
        <attr name="pickerview_lineSpacingMultiplier" format="float"/>
    </declare-styleable>
</resources>